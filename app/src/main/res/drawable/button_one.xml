<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#F1948A" />
            <gradient android:angle="-90" android:startColor="#D1F2EB" android:endColor="#A9CCE3"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#B2BABB" />
            <solid android:color="#BDC3C7"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#95A5A6" />
            <gradient android:angle="-90" android:startColor="#95A5A6" android:endColor="#B2BABB" />
        </shape>
    </item>

</selector>